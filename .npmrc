# Força instalação de dependências opcionais
optionalDependencies=true

# Resolve problemas com dependências binárias
force=false

# Configurações para melhor compatibilidade com ambientes de CI/CD
prefer-offline=false
audit=false

# Evita problemas com peer dependencies
legacy-peer-deps=false

# Configurações para Cloudflare Pages
engine-strict=false
fund=false
